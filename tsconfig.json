{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@i18n/*": [
        "./src/i18n/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "src",
    "astro.config.ts"
  ],
}